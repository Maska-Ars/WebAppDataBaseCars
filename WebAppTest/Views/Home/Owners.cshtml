@using WebAppTest.Models
@model IEnumerable<Owner>

@{
    ViewData["Title"] = "Таблица: Машины";
}
<h5>Таблица: Машины</h5>

<table border="1">
    <thead>
        <th style="border: 1px outset rgb(0, 0, 0);">Номер</th>
        <th style="border: 1px outset rgb(0, 0, 0);">Фамилия</th>
        <th style="border: 1px outset rgb(0, 0, 0);">Имя</th>
        <th style="border: 1px outset rgb(0, 0, 0);">Отчество</th>
    </thead>
    <tbody>
        @foreach (Owner car in Model)
        {
            <tr>
                <td style="border: 1px outset rgb(0, 0, 0);">@car.Number</td>
                <td style="border: 1px outset rgb(0, 0, 0);">@car.SecondName</td>
                <td style="border: 1px outset rgb(0, 0, 0);">@car.Name</td>
                <td style="border: 1px outset rgb(0, 0, 0);">@car.Surname</td>
            </tr>
        }
    </tbody>
</table>
